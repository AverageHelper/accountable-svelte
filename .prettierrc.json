{
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "strict",
	"printWidth": 100,
	"proseWrap": "preserve",
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"useTabs": true,
	"svelteAllowShorthand": true,
	"svelteIndentScriptAndStyle": true,
	"svelteSortOrder": "options-scripts-markup-styles",
	"svelteBracketNewLine": true
}
