{
	"name": "accountable-svelte",
	"version": "0.10.2",
	"license": "GPL-3.0",
	"description": "A Svelte app for managing monetary assets.",
	"scripts": {
		"start": "./node_modules/.bin/concurrently -rkn SERVER,APP -s first \"npm run dev:server\" \"npm run dev:client\"",
		"serve": "./node_modules/.bin/vite preview --open",
		"test": "jest",
		"test:watch": "npm run test -- --watch",
		"dev:client": "./node_modules/.bin/vite --open",
		"dev:server": "cd ./server && npm start",
		"prod:server": "NODE_ENV=production node ./server",
		"build": "npm run build:client && npm run build:server",
		"build:server": "cd ./server && npm run build",
		"build:client": "rm -rf ./dist && npm run lint && npm run svelte-check && npm run vite:build",
		"build:client:quick": "rm -rf ./dist && npm run export-version && npm run vite:build",
		"svelte-check": "./node_modules/.bin/svelte-check --tsconfig tsconfig.prod.json --use-new-transformation --compiler-warnings 'css-unused-selector:ignore,a11y-label-has-associated-control:ignore'",
		"vite:build": "./node_modules/.bin/vite build",
		"lint": "npm run export-version && npm run lintonly",
		"lint:fix": "npm run export-version && npm run lintonly -- --fix",
		"lintonly": "./node_modules/.bin/eslint . --ext .js,.ts",
		"export-version": "./node_modules/.bin/genversion ./src/version.ts -es && cd server && npm run export-version"
	},
	"type": "module",
	"dependencies": {
		"@averagehelper/job-queue": "1.0.1",
		"@zip.js/zip.js": "2.4.26",
		"atob-lite": "2.0.0",
		"bootstrap": "5.2.0",
		"btoa-lite": "1.0.0",
		"crypto-js": "4.1.1",
		"date-fns": "2.28.0",
		"dinero.js": "2.0.0-alpha.8",
		"fuse.js": "6.6.2",
		"lodash": "4.17.21",
		"source-map-support": "0.5.21",
		"superstruct": "0.15.5",
		"svelte-i18n": "3.4.0",
		"svelte-navigator": "3.1.6",
		"svelte-portal": "2.2.0",
		"uuid": "8.3.2"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "8.3.3",
		"@sveltejs/vite-plugin-svelte": "1.0.1",
		"@types/atob-lite": "2.0.0",
		"@types/btoa-lite": "1.0.0",
		"@types/crypto-js": "^4.0.2",
		"@types/jest": "^27.0.2",
		"@types/lodash": "4.14.182",
		"@types/source-map-support": "0.5.4",
		"@types/uuid": "8.3.4",
		"@typescript-eslint/eslint-plugin": "5.33.0",
		"@typescript-eslint/parser": "5.33.0",
		"@zerodevx/svelte-toast": "0.7.2",
		"autoprefixer": "^10.4.2",
		"concurrently": "^6.4.0",
		"eslint": "8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-strictest": "^0.4.0",
		"eslint-plugin-deprecation": "^1.3.2",
		"eslint-plugin-jest": "^26.5.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"genversion": "^3.0.2",
		"jest": "^27.3.1",
		"jest-environment-node": "^27.3.1",
		"jest-extended": "^1.1.0",
		"postcss": "^8.4.5",
		"prettier": "^2.6.2",
		"rollup-plugin-analyzer": "4.0.0",
		"sass": "1.53.0",
		"sass-mq": "6.0.0",
		"svelte": "3.49.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"ts-jest": "^27.0.7",
		"typescript": "4.7.3",
		"vite": "3.0.2",
		"vite-tsconfig-paths": "3.5.0"
	}
}
